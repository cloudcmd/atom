{
  "name": "cloudcmd-electron",
  "productName": "cloudcmd",
  "version": "6.7.0",
  "private": true,
  "changelog": false,
  "release": false,
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Desktop version of Cloud Commander",
  "homepage": "http://cloudcmd.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/cloudcmd-app.git"
  },
  "dependencies": {
    "cloudcmd": "*"
  },
  "main": "lib/main.js",
  "scripts": {
    "lint": "eslint lib",
    "pack:windows": "electron-packager . --platform=win32 --arch x64 --icon=icon.ico",
    "mv:windows": "mv cloudcmd-win32-x64 cloudcmd-v`version`-win32-x64",
    "zip:windows": "onezip -p cloudcmd-v`version`-win32-x64",
    "rm:windows": "rimraf cloudcmd-*",
    "rm:modules": "rimraf node_modules",
    "install:all": "npm i",
    "upload:windows": "putasset -u cloudcmd -r electron -t v`version` -f cloudcmd-v`version`-win32-x64.zip",
    "dist": "redrun rm:* install:* pack:* mv:* zip:* upload:*",
    "dist:ci": "redrun rebuild:* pack:* mv:* zip:* upload:*",
    "wisdom:done": "npm run dist",
    "rebuild:electron" : "electron-rebuild -f -w node-pty"
  },
  "devDependencies": {
    "electron": "^1.4.6",
    "electron-packager": "^8.2.0",
    "eslint": "^3.10.0",
    "onezip": "^1.0.4",
    "putasset": "^2.0.0",
    "rimraf": "^2.5.4",
    "version-io": "^1.2.5"
  }
}
